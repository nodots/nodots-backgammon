{
  "name": "@nodots-llc/backgammon-api",
  "version": "1.0.0",
  "description": "REST API for Nodots Backgammon with Auth0 authentication",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg"
  },
  "dependencies": {
    "@nodots-llc/backgammon-core": "file:../nodots-backgammon-core",
    "@nodots-llc/backgammon-types": "file:../nodots-backgammon-types",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.4",
    "drizzle-orm": "^0.29.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-client": "^2.0.0",
    "postgres": "^3.4.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "selfsigned": "^2.4.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "backgammon",
    "api",
    "auth0",
    "game"
  ],
  "author": "Ken Riley <kenr@nodots.com>",
  "license": "MIT"
}
